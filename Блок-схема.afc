      <branch>
        <process text="s:=s+1"/>
        <process text="Правильно"/>
      </branch>
      <branch>
        <process text="Не правильно"/>
      </branch>
    </if>
    <ou vars="Общее чило баллов за тест:"/>
    <ou vars="s"/>
    <if cond="0..5">
      <branch>
        <process text="Твои знания по истории нулевые... Иди учи!"/>
      </branch>
      <branch>
        <if cond="6..8">
          <branch>
            <process text="Твои знания по истории средние. Тебе стоит повторить!"/>
          </branch>
          <branch>
            <if cond="9..10">
              <branch>
                <process text="Твои знания по истории великолепны! Иди отдыхай!"/>
              </branch>
              <branch/>
            </if>
          </branch>
        </if>
      </branch>
    </if>
    <ou vars="---------- Ключ к тесту: ----------"/>
    <ou vars="1 - 2,2 - 3,3 - 1,4 - 2,5 - 1,6 - 1,7 - 1,8 - 1,9 - 2,10 - 2"/>
  </branch>
</algorithm>
